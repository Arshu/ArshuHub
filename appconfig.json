{
  "WebRootName": "wwwroot",
  "ConfigLogLevel": "Error,Warn",
  "ConfigPorts": false,
  "ConfigHttps": true,

  "CreateResourceConfig": false,
  "ReadApiConfigPatternList": "*",
  "CreateApiConfigPatternList": "Server,Metrics",
  "AllowApiConfigUpdate": true,
  "DisablePowChecking": true,
  "DisablePowCheckingPerPage": true,

  "LoadArshuFramework": true,
  "LoadAppHub": true,
  "LoadAppData": true,
  "LoadAppConfig": true,
  "LoadAppEmbed": false,
  "LoadAppFolder": true,
  "LoadAppPap": true,
  "LoadFileChanges": false,

  "RealtimeAppSites": "",
  "StaticAppSites": "Index, Account, Test, Hub",
  "SearchAppSites": "Account, Index, Hub",

  "HubUseWebFolder": true,
  "HubRootDirName": "wwwhub",
  "HubWebDirName": "App_Data/Hub",

  "ContextJson": {
    "WebMetricsID": "aeadbc05-9dfe-4ec5-bf95-5a9aa13b2d7a",
    "PageTitle": "Application Base",

    "ShowIntroTip": true,
    "ShowFrameworkToolbar": true,
    "ShowFrameworkView": true,
    "ShowFrameworkAPI": true,
    "ShowTestLink": true,
    "ShowHubLink": true,
    "ShowHostLink": true,

    "ShowDebug": true,
    "DefaultDebug": true,
    "DefaultRecordMetrics": false
  },

  "AppAssets": [
    {
      "AssetTypeName": "Arshu.Hub.AssetRegister",
      "AssetDll": "Arshu.Hub.dll",
      "IsEnabled": true
    }
  ],

  "AppServices": [
    {
      "RegisterTypeName": "Arshu.AppCore.App.Register.WebRegister",
      "RegisterDll": "Arshu.AppCore.dll",
      "IsEnabled": true
    },
    {
      "RegisterTypeName": "Arshu.AppCore.App.Register.VueRegister",
      "RegisterDll": "Arshu.AppCore.dll",
      "IsEnabled": true
    },
    {
      "RegisterTypeName": "Arshu.AppServer.App.Register.MetricsRegister",
      "RegisterDll": "Arshu.AppServer.dll",
      "IsEnabled": true
    },
    {
      "RegisterTypeName": "Arshu.AppServer.App.Register.ServerRegister",
      "RegisterDll": "Arshu.AppServer.dll",
      "IsEnabled": true
    },
    {
      "RegisterTypeName": "Arshu.AppHub.App.Register.HubRegister",
      "RegisterDll": "Arshu.AppHub.dll",
      "IsEnabled": true
    }
  ],

  "AutoStopCheckTime": 2,
  "LogServerMetrics": true,
  "LogUsageTracking": true,
  "LogUsageTimeInSecs": 30,
  "AutoManageMachines": true,
  "AutoDestroyMachines": true,
  "AutoUpdateDeployed": false,
  "AutoManageRemoteApi": false,
  "AutoMachineConfig": "Basic_1vCPU-256MB",
  "MachineSuspend": false,

  "EchoHandler": true,
  "ReplayHeader": true,
  "OnlyRootGET": false,
  "AllowedGET": ".js,.css, .png,.jpg,.jpeg,.svg,.ico,.webp,.webmanifest",
  "MetricPort": 0,

  "AllowReload": true,
  "PublishMetaData": true,
  "PersistMetricLog": true,
  "SaveJsCssResource": false,
  "SaveJsCssResourceGroup": false,

  "WriteMetaDataToFile": false,
  "WriteMetaDataToFileOnGET": false,
  "WriteMetaDataToFileWithTime": false,

  "AppProviders": [

    {
      "RegisterTypeName": "Arshu.Data.Register.DataRegister",
      "RegisterDll": "Arshu.Data.dll",
      "IsEnabled": false,
      "ProviderTypeName": "Microsoft.Data.Sqlite.SqliteFactory",
      "ProviderDll": "Microsoft.Data.Sqlite.dll"
    }
  ],

  "PackWebRootName": "wwwsys",
  "SkipSystemDirectories": "_Meta, .git, Log, bin, obj, node_modules, _bin_deployableAssemblies, Metrics, Bak, Backup, Repository, Cache, Inbox, InboxLocal, InboxRemote",
  "SystemDirectories": "App_Config, App_Data, Db, Backup, Output, UndoRedo, Cache",
  "AllowDirectories": "",
  "SkipAppDirectories": "Arshu, Templates, Resource, Content, App_Config, App_Data, AppApi, Backup, Output, UndoRedo, Cache",
  "SkipResourceDirectories": "Arshu, Component, AppSites, Templates, App_Config, App_Data, Backup, Output, UndoRedo, AppApi, Dynamic, Meta, Upload, Cache",
  "SkipDataDirectories": "Arshu, AppSites, Component, Templates, Resource, Content, Backup, Output, UndoRedo, Cache, Manifest, Layer, Metadata",

  "PackAppDirectories": "AppSites, AppApi",
  "PackResourcesDirectories": "Resource,Content",
  "PackConfigDirectories": "App_Config, AppApi",
  "PackDataDirectories": "App_Data, Db",

  "PackApp": true,
  "PackResource": true,
  "PackConfig": true,
  "PackData": true,
  "PackAppSites": "Index, Hub",
  "PackAppConfigs": "App_Config, AppApi",
  "PackAppDatas": "Cert, SSL, Roles, Members",

  "PackName": "Arshu.Hub.1.0.pap",
  "PackDescription": "Arshu.Hub for Managing Application Hub",
  "PackResourceName": "Arshu.Hub.1.0.pap",
  "PackResourceDescription": "Resource of Arshu.Hub for Managing Application Hub",
  "PackConfigName": "Arshu.Hub.1.0.pap",
  "PackConfigDescription": "Config of Arshu.Hub for Managing Application Hub",
  "PackDataName": "Arshu.Hub.1.0.pap",
  "PackDataDescription": "Data of Arshu.Hub for Managing Application Hub",

  "PackAppSubFolderPath": "A0",
  "PackResourceSubFolderPath": "A0",
  "PackConfigSubFolderPath": "A0",
  "PackDataSubFolderPath": "A0",

  "SkippedFileExtension": "dll, config, exe, pap, so, map, txt",
  "AllowedAppFileExtension": "Html, Json",
  "AllowedResourceFileExtension": "Bmp, Png, Jpg, Jpeg, Gif, Ico, woff, woff2, otf, eot, svg, ttf, Js, Mjs, Css, json, webmanifest",
  "AllowedConfigFileExtension": "json, xml, pfx, xlsx",
  "AllowedDataFileExtension": "dat, json, xml, pem, pfx, xlsx, db, sdf",

  "ResourceOptimizeMergeJs": true,
  "ResourceOptimizeMergeJsPattern": "arshu; highlight, chart, main",
  "ResourceOptimizeMergeJsSkipPattern": "",
  "ResourceOptimizeMergeCss": true,
  "ResourceOptimizeMergeCssPattern": "normalize, arshu, animate, highlight, chart, main; ",
  "ResourceOptimizeMergeCssSkipPattern": ""
}